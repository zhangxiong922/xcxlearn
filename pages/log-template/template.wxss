.box {
  display: flex;
  flex-direction: column;
  margin-top: 20rpx;
  border-top: 1px solid #ccc;
  border-bottom: 1px solid #ccc;
  padding-bottom: 10rpx;
}
.list1 {
  padding: 10rpx;
}
.list1 image{
    width: 60rpx;
    height: 60rpx;
    vertical-align: middle;
    margin-right: 20rpx;
    border-radius: 50%;
}
.list1 text {
  font-size: 28rpx;
}
.list2 {
  font-weight: bold;
  font-size: 36rpx;
  margin: 10rpx 4rpx 10rpx 0;
}
.list3 image {
  width: 100%;
  height: 340rpx;
}
.list4 {
  color: #666;
  line-height: 56rpx;
  text-indent: 32rpx;
  font-size: 32rpx;
}
.list5 {
  margin-top: 10rpx;
}
.list5 image {
  width: 32rpx;
  height: 32rpx;
  margin-right: 10rpx;
}
.list5 text {
  color: #666;
  margin-right: 10rpx;
}